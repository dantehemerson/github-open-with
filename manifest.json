{
  "manifest_version": 2,
  "name": "Gihubbox",
  "short_name": "Githubbox",
  "version": "0.1",
  "description": "A browser extension to open repositories with multiple online text editors",
  "icons": {
    "16": "icons/extension-logo-16.png",
    "48": "icons/extension-logo-48.png",
    "128": "icons/extension-logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/bundles/index.bundle.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/extension-logo-16.png",
      "48": "icons/extension-logo-48.png",
      "128": "icons/extension-logo.png"
    },
    "default_title": "Githubbox"
  },
  "permissions": ["storage"]
}
